<meta charset="utf-8">
<link rel="dns-prefetch" href="https://fonts.gstatic.com/">

<meta http-equiv="content-type" content="text/html; charset=utf-8">
<meta name="language" content="en">
<meta name="viewport" content="width=device-width, initial-scale=1.0">

<meta http-equiv="X-UA-Compatible" content="IE=edge">

@if(!String.IsNullOrEmpty(@Model.GetString("Author")))
{
<meta name="author" content="@Model.GetString("Author")">
}
@if(!String.IsNullOrEmpty(@Model.GetString("Description")))
{
<meta name="description" content="@Model.GetString("Description")">
<meta property="og:description" content="@Model.GetString("Description")" />
<meta name="twitter:description" content="@Model.GetString("Description")">
}
@if(!String.IsNullOrEmpty(@Model.GetString("Keywords")))
{
<meta name="keywords" content="@Model.GetString("Keywords")" />
}
<meta name="copyright" content="@Document.GetString(WebKeys.Copyright)" />

<title>@Document.GetString("PageTitle")</title>

<meta property="og:title" content="@Document.GetString("PageTitle")" />
<meta property="og:site_name" content="@Document.GetString("PageTitle")" />
<meta property="og:type" content="Website" />
<meta property="og:image" content="@Document.GetString(WebKeys.Image)" />
<meta property="og:url" content="@Document.GetLink(true)" />

<meta name="twitter:title" content="@Document.GetString("PageTitle")">
<meta name="twitter:image" content="@Document.GetString(WebKeys.Image)">
<meta name="twitter:card" content="summary_large_image">

<link href="/vendor/bootstrap/bootstrap.min.css" rel="stylesheet">
<link rel="preconnect" href="https://fonts.gstatic.com">
<link href="https://fonts.googleapis.com/css2?family=Raleway:wght@300;400;500&display=swap" rel="stylesheet">
<link href="@Context.GetLink("/scss/styles.css")"rel="stylesheet">